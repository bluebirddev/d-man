{
  "name": "d-man",
  "version": "0.3.5",
  "description": "Apollo inspired library to connect redux, local-storage, and rest domains seamlessly.",
  "author": "vssrcj",
  "license": "MIT",
  "repository": "https://github.com/bluebirddev/d-man",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "lint": "tsc --noEmit && eslint src/**/*.{js,jsx,ts,tsx}",
    "lint:fix": "eslint src/**/*.{js,jsx,ts,tsx} --fix",
    "start:example": "npm run predeploy && npm run start"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-redux": ">=7",
    "redux": ">=4",
    "uuid": ">=8",
    "ramda": "^0.27",
    "axios": "^0.20",
    "reselect": ">=4",
    "date-fns": ">=2"
  },
  "devDependencies": {
    "@types/node": "^14.14.25",
    "@types/ramda": "^0.27.36",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.16",
    "@types/uuid": "^8.3.0",
    "axios": "^0.21.1",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.3",
    "date-fns": "^2.17.0",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-typescript-prettier": "^4.1.0",
    "gh-pages": "^3.1.0",
    "microbundle-crl": "^0.13.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "ramda": "^0.27.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-scripts": "^4.0.2",
    "redux": "^4.0.5",
    "reselect": "^4.0.0",
    "typescript": "^4.1.3",
    "uuid": "^8.3.2"
  },
  "files": [
    "dist"
  ]
}
